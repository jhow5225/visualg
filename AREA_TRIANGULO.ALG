Algoritmo "area_triangulo"

Var

   base: real
   altura: real
   area: real

Inicio
  escreval ("Digite a base do triângulo (cm):")
  leia (base)
  
  escreval ("Digite a altura do triângulo (cm):")
  leia (altura)
  
  area<- (base * altura)/2
  
  escreval ("A área do triângulo é:", area, "cm")


Fimalgoritmo